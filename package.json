    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "jest-styled-components": "^7.1.1",
    "prettier": "^2.8.0",
    "react-app-rewired": "^2.2.1",
    "react-scripts": "^5.0.1",
